<div class="d-flex justify-content-between align-items-center">
  <div class="d-flex gap-2 align-items-center">
      <%= image_tag history.performer.avatar, title: history.performer.full_name, class: "w-[70px] h-[70px] rounded-full" if history.performer.avatar.attached?%>
      <div class="flex gap-2">
        <div>
          <h5 class="mb-0 tracking-tight text-gray-900 dark:text-white"><%=history.object_activity_type%></h5>
          <p class="mb-0 font-normal text-gray-500 dark:text-gray-400"><%= time_ago_in_words(history.created_at) %></p>
        </div>
      </div>
    </div>
  </div>
</div>
